{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": false,
        "loose": true,
        "shippedProposals": true,
        "targets": {
          "browsers": ["last 4 versions", "safari >= 7", "ie >= 9"]
        },
        "modules": "commonjs"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-transform-runtime"
  ]
}
